const uint8_t *array[] = {cheer,happy,urcool}; 
void draw(const uint8_t *pixelart){
  int counter=0;
  for(int y = 0;y<32;y++){
    for(int x = 0; x <32 ; x++){
      uint8_t r = pgm_read_word_near(pixelart+counter);
      uint8_t g = pgm_read_word_near(pixelart+counter+1);
      uint8_t b = pgm_read_word_near(pixelart+counter+2);
      matrix.drawPixel(x,y,matrix.Color888(r,g,b,1));
      counter+=3;
    }//end x-loop
  }//end y-loop


}

void wait(){
  int first = millis();
  int last = millis();
  while( last - first < heartTimer){
    last = millis();
    if(digitalRead(button)==HIGH){
      draw(array[random(3)]);
      delay(2000);
      
    }
  }
}

void setup() {
  // put your setup code here, to run once:
  matrix.begin();
   matrix.fillScreen(0);
  matrix.setCursor(0,0);
  //matrix.drawPixel(0,0,pokemon[0]);
  pinMode(buttonSource,OUTPUT);
  pinMode(button,INPUT);
  digitalWrite(buttonSource,HIGH);
}

void loop() {
  // put your main code here, to run repeatedly:
    draw(heart1);
    wait();
    draw(heart2);
    wait();    
    draw(heart3);
    wait();
    draw(heart4);
    wait();
    draw(heart3);
    wait();
    draw(heart2);
    wait();
    draw(heart1);
    wait();
    
 
}
